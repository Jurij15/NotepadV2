<Window x:Class="NotepadV2.Windows.StatisticsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NotepadV2.Windows"
        mc:Ignorable="d"
        WindowStartupLocation="CenterOwner"
        Title="App Statistic" Height="286" Width="542" ResizeMode="NoResize" Closing="Window_Closing"
                         xmlns:ui="http://schemas.modernwpf.com/2019"
        ui:WindowHelper.UseModernWindowStyle="True">
    <ListBox x:Name="AllStatsBox" d:ItemsSource="{d:SampleData ItemCount=5}"/>
</Window>
